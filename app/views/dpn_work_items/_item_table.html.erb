<table class="table">
  <thead>
  <tr>
    <th>Identifier</th>
    <th>Remote Node</th>
    <th>Task</th>
    <th>Queued At</th>
    <th>Completed At</th>
  </tr>
  </thead>
  <tbody>
  <% @paged_results.each do |item| %>
      <tr>
        <td><%= link_to item.identifier, dpn_work_item_path(item.id) %></td>
        <td><%= item.remote_node %></td>
        <td><%= item.task %></td>
        <td><%= item.queued_at %></td>
        <td><%= item.completed_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>