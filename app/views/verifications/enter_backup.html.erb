<h4>Please enter one of your back up codes.</h4>

<%= form_tag check_backup_verification_path(id: params[:id]), method: :put do %>
    <div class="form-group">
      <%= label_tag :code %><br />
      <%= text_field_tag :code %>
    </div>
    <%= submit_tag 'Verify Backup Code', class: "btn btn-primary" %>
<% end %>