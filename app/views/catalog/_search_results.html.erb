

<% counter = 0 %>
<% @paged_results.each do |current| %>
  <% counter = counter+1 %>
  <% if current.is_a?(IntellectualObject) %>
    <%= render 'object_result', counter: counter, current: current %>
  <% elsif current.is_a?(GenericFile) %>
    <%= render 'file_result' %>
  <% elsif current.is_a?(WorkItem) %>
    <%= render 'item_result' %>
  <% end %>
<% end %>