wb = xlsx_package.workbook
styles = xlsx_package.workbook.styles
header_style = styles.add_style bg_color: '00',
                                fg_color: 'FF',
                                bold: true,
                                alignment: { horizontal: :center }
wb.add_worksheet(name: 'Snapshots') do |sheet|
  sheet.add_row ['Institution', 'Bytes Preserved'], style: header_style
  @snap_hash.each do |name, bytes|
    sheet.add_row [name, bytes]
  end
end